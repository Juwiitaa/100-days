package day48;
public class Main {
	public static void main(String[] args) {
		
		System.out.println("===== Menghitung nilai rata-rata =====\n\n== Nilai ==");
		
		int[] nilai = {80,81,83,85,85};
		
		int total = 0;
		
		for (int nilai2 : nilai){
			total += nilai2;
			System.out.print(nilai2+",");
		}
		
		double nilaiRata = total / nilai.length;
		System.out.print("\n\nRata-rata dari nilai tersebut adalah = "+nilaiRata+"\n\n===== Semangat =====");
		
	}
}
